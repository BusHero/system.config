      <maml:alert>
        <maml:para></maml:para>
      </maml:alert>
    </maml:alertSet>
    <command:examples>
      <command:example>
        <maml:title>-------------- Example 1: Disable Developer Mode --------------</maml:title>
        <dev:code>PS C:\&gt; Unregister-WindowsDeveloperLicense</dev:code>
        <dev:remarks>
          <maml:para>This command disables Developer Mode for the current computer. The cmdlet prompts you for confirmation before it disables Developer Mode. To skip this prompt, specify the Force parameter.</maml:para>
        </dev:remarks>
      </command:example>
