    <command:details>
      <command:name>Get-SmbDelegation</command:name>
      <command:verb>Get</command:verb>
      <command:noun>SmbDelegation</command:noun>
      <maml:description>
        <maml:para>Gets the constrained delegation authorizations for an SMB client.</maml:para>
      </maml:description>
