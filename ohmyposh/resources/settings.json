{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf0e7",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#ffff66",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{.Icon}}: ",
          "foreground": "#ffffff",
          "background": "#ffffff",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_locations": {
              "C:\\temp": "\ue799"
            },
            "style": "agnoster"
          },
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#ffffff",
          "background": "#ff479c",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#fffb38",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#6CA35E",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " \ue23a{{ if eq .Code 0 }} \uf42e{{ else }} \uf00d{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#2e9599",
          "type": "status",
          "style": "powerline",
          "background_templates": [
            "{{ if ne .Code 0 }}#f1184c{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": ">",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
